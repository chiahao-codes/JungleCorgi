                animationId = requestAnimationFrame(animation);
                if (chevronDirection) {
                    if (currSlideIndex < slides.length - 1) {
                        currSlideIndex += 1
                    }
                }

                //prev button
                if (!chevronDirection) {
                    if (currSlideIndex > 0) {
                        currSlideIndex -= 1;
                    }
                }


                isDragging = false;
                cancelAnimationFrame(animationId);
                setPostionByIndex()
            })
