            console.log("response:", response)

            currPrice = response.chart.result[0].meta.regularMarketPrice;
            previousClose = '<%=previousClose%>'
            console.log("previous close:", previousClose)

            //set session
            if (range === "1d") {
                setSession(response).then(() => formatPrice(regPointsPctContainer)).then(() => formatPrice(afterPointsPctContainer))
