                </div>
                <div class="border-line">
                    <div id="fifty2WkLow">52 Week Low</div>
                    <% let fifty2WkLo="N/A" %>
                        <% if(checkSymbol.summaryDetail){ %>
                            <% if(checkSymbol.summaryDetail.fiftyTwoWeekLow){ %>
                                <% fifty2WkLo=checkSymbol.summaryDetail.fiftyTwoWeekLow} }%>
