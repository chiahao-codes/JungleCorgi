    <script>
        const pageUrl = window.location.href;
        localStorage.setItem("url", pageUrl);
        const apiKey = '<%=API_KEY%>';
        const stockTicker = '<%=checkSymbol.price.symbol%>';
        const dividendRate = '<%=checkSymbol.summaryDetail.dividendRate%>'
